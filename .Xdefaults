URxvt*loginShell:true
URxvt*transparent:false
## URxvt*shading:25
URxvt*scrollBar:false
URxvt*scrollBar_right:true
URxvt*scrollBar_floating:false
URxvt*scrollstyle:plain
URxvt*secondaryScroll:true
URxvt*.internalBorder: 15

URxvt*foreground: white
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800

URxvt*color0:  #2E3436
URxvt*color1:  #a40000
URxvt*color2:  #4E9A06
URxvt*color3:  #C4A000
URxvt*color4:  #3465A4
URxvt*color5:  #75507B
URxvt*color6:  #ce5c00
URxvt*color7:  #babdb9
URxvt*color8:  #555753
URxvt*color9:  #EF2929
URxvt*color10: #8AE234
URxvt*color11: #FCE94F
URxvt*color12: #729FCF
URxvt*color13: #AD7FA8
URxvt*color14: #fcaf3e
URxvt*color15: #EEEEEC

URxvt*saveLines:32767
URxvt*troughColor:#000000
URxvt*scrollColor:#222222
URxvt*font: xft:inconsolata:pixelsize=14:antialias=true, \
            xft:Ubuntu Mono:pixelsize=14:antialias=true, \
            xft:Symbola:pixelsize=14:antialias=true
URxvt*letterSpace: 1
URxvt*lineSpace: 1

URxvt*urgentOnBell: true

URxvt*perl-ext-common: default,matcher,-option-popup,-selection-popup,-realine,clipboard
URxvt*matcher.button: 1
URxvt*urlLauncher: firefox

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False

Xcursor.theme: Adwaita

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   #ffffff,#525d76,#f5f5f5,#5294e2,#ffffff
! Color scheme for urgent row
rofi.color-urgent:                   #ffffff,#dc322f,#f5f5f5,#5294e2,#dc322f
! Color scheme for active row
rofi.color-active:                   #ffffff,#c2cad0,#f5f5f5,#5294e2,#c2cad0
! Color scheme window
rofi.color-window:                   #ffffff,#f5f5f5
! Separator style (none, dash, solid)
rofi.separator-style:                solid
